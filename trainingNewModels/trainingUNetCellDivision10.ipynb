    "# Hyperparameters\n",
    "\n",
    "LEARNING_RATE = 1e-4\n",
    "DEVICE = \"cuda\" if torch.cuda.is_available() else \"cpu\"\n",
    "BATCH_SIZE = 1\n",
    "NUM_EPOCHS = 4\n",
    "NUM_WORKERS = 2\n",
    "IMAGE_HEIGHT = 512\n",
    "IMAGE_WIDTH = 512\n",
    "PIN_MEMORY = True\n",
    "LOAD_MODEL = True\n",
    "TRAIN_IMG_DIR = \"dat/train_images/\"\n",
    "TRAIN_MASK_DIR = \"dat/train_masks/\"\n",
    "VAL_IMG_DIR = \"dat/val_images/\"\n",
    "VAL_MASK_DIR = \"dat/val_masks/\"\n"
